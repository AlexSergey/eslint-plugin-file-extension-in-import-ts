    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^43.0.1",
    "git-is-ready-to-push": "^1.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
